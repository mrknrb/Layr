{
  "manifest_version": 2,
  "name": "MrkS3",
  "description": "MrkS 4ever!",
  "version": "0.0.0",
  "icons": {
    "128": "0Resources/Asset128.png",
    "48": "0Resources/Asset48.png",
    "16": "0Resources/Asset16.png"
  },
  "chrome_url_overrides": {
    "newtab": "Main.html"
  },
  "browser_action": {
    "default_icon": "0Resources/Asset48.png",
    "default_title": "MrkS",
    "persistent": true
  },
  "background": {
    "scripts": [
      "0Libraries/jquery.js",
      "0Libraries/arangojs.js",
      "0Libraries/pouchdb.js",
      "backgroundStarterSeged.js"
    ],
    "persistent": true
  },
  "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "https://www.google.hu/search?*",
    "https://www.google.com/search?*",
    "https://i.reddit.com/*",
    "http://*/*",
    "https://*/*",
    "unlimitedStorage",
    "<all_urls>",
    "cookies",
    "history",
    "https://www.reddit.com/*",
    "webNavigation"
  ]
}