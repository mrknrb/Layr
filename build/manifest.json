{
  "manifest_version": 2,
  "name": "Layr",
  "description": "Layr extension",
  "version": "0.0.0",
  "icons": {
    "128": "0Resources/LayrLogo16.png",
    "48": "0Resources/LayrLogo48.png",
    "16": "0Resources/LayrLogo128.png"
  },
  "chrome_url_overrides": {
    "newtab": "Frame.html"
  },
  "browser_action": {
    "default_icon": "0Resources/LayrLogo48.png",
    "default_title": "Layr",
    "persistent": true
  },
  "background": {
    "scripts": [
      "0Libraries/jquery.js",
      "0Libraries/arangojs.js",
      "0Libraries/pouchdb.js",
      "Egyebek/BackgroundStarterSeged.js"
    ],
    "persistent": true
  },
  "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "https://www.google.hu/search?*",
    "https://www.google.com/search?*",
    "https://i.reddit.com/*",
    "http://*/*",
    "https://*/*",
    "unlimitedStorage",
    "<all_urls>",
    "cookies",
    "history",
    "https://www.reddit.com/*",
    "webNavigation"
  ]
}